{
  "name": "cs-demo-downloader",
  "version": "1.0.0",
  "description": "Automatically download Counter-Strike demos of ranked, unranked, and wingman matches",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader tsx src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "tsc --noEmit && eslint .",
    "docker:build": "docker build . -t cs-demo-downloader:latest --target deploy",
    "docker:run": "docker run --rm -ti -v $(pwd)/config:/config -v $(pwd)/demos:/demos cs-demo-downloader:latest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "p-queue": "^7.4.1",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "steam-session": "^1.3.0",
    "steam-totp": "^2.1.2",
    "steam-user": "^4.29.1",
    "unbzip2-stream": "^1.4.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.1",
    "@tsconfig/strictest": "^2.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.12",
    "@types/steam-totp": "^2.1.0",
    "@types/steam-user": "^4.26.4",
    "@types/unbzip2-stream": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}